<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    <!-- White X/Cross symbol - Clean and simple design -->
    <group android:translateX="54"
        android:translateY="54">
        <!-- First diagonal stroke (top-left to bottom-right) -->
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="8"
            android:strokeLineCap="round"
            android:pathData="M-20,-20 L20,20"/>
        <!-- Second diagonal stroke (top-right to bottom-left) -->
        <path
            android:strokeColor="#FFFFFF"
            android:strokeWidth="8"
            android:strokeLineCap="round"
            android:pathData="M20,-20 L-20,20"/>
    </group>
</vector>