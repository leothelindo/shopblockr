    <string name="accessibility_service_description">ShopBlockr helps protect you from shopping distractions by detecting sponsored content and shopping elements in TikTok. This service needs to read screen content to identify shopping-related elements and show protective warnings.</string>
    <string name="app_name">ShopBlockr</string>
    <style name="Base.Theme.ShopBlockr" parent="Theme.Material3.DayNight.NoActionBar">
        
        <item name="colorPrimary">@color/accent_primary</item>
        <item name="colorOnPrimary">@color/text_on_primary</item>
        <item name="colorPrimaryContainer">@color/accent_primary_container</item>
        <item name="colorOnPrimaryContainer">@color/text_primary</item>
        
        
        <item name="colorSecondary">@color/accent_secondary</item>
        <item name="colorOnSecondary">@color/text_on_primary</item>
        <item name="colorSecondaryContainer">@color/accent_secondary_container</item>
        <item name="colorOnSecondaryContainer">@color/text_primary</item>
        
        
        <item name="colorSurface">@color/surface</item>
        <item name="colorOnSurface">@color/text_on_surface</item>
        <item name="colorSurfaceVariant">@color/surface_variant</item>
        <item name="colorOnSurfaceVariant">@color/text_secondary</item>
        
        
        <item name="android:colorBackground">@color/background_primary</item>
        <item name="colorOnBackground">@color/text_primary</item>
        
        
        <item name="colorError">@color/error</item>
        <item name="colorOnError">@color/text_on_primary</item>
        <item name="colorErrorContainer">@color/error_container</item>
        <item name="colorOnErrorContainer">@color/error</item>
        
        
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:textColorSecondary">@color/text_secondary</item>
        <item name="android:textColorTertiary">@color/text_tertiary</item>
        
        
        <item name="android:statusBarColor">@color/background_primary</item>
        <item name="android:windowLightStatusBar">true</item>
        
        
        <item name="android:windowBackground">@color/background_primary</item>
        
        
        <item name="colorSwitchThumbNormal">@color/switch_thumb_disabled</item>
        <item name="colorControlActivated">@color/switch_thumb_enabled</item>
        <item name="android:colorControlHighlight">@color/switch_track_enabled</item>
    </style>
    <style name="ShopBlockr.Switch" parent="Widget.Material3.CompoundButton.Switch">
        <item name="android:thumb">@drawable/switch_thumb</item>
        <item name="android:track">@drawable/switch_track</item>
        <item name="thumbTint">@null</item>
        <item name="trackTint">@null</item>
    </style>
    <style name="TextAppearance.ShopBlockr.Body" parent="TextAppearance.Material3.BodyMedium">
        <item name="android:textColor">@color/text_secondary</item>
        <item name="android:lineSpacingExtra">2dp</item>
    </style>
    <style name="TextAppearance.ShopBlockr.Headline" parent="TextAppearance.Material3.HeadlineLarge">
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="TextAppearance.ShopBlockr.Title" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textColor">@color/text_primary</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="Theme.ShopBlockr" parent="Base.Theme.ShopBlockr"/>
</resources>